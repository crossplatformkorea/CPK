{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "channel": "preview",
      "extends": "production",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "ENVIRONMENT": "production"
      },
      "channel": "production",
      "cache": {
        "disabled": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
